class test:
	int myvar
	this():
		myvar = 5
	protected:
		abstract method addto( int a -> void ):
			myvar += a

class another(T):
	T myvar

	method addto( T a ):
		myvar += a

class tester <- test:
	this( int x ):
		super()
		myvar = x
	override:
		method addto( int a -> void ):
			myvar += a + 1

procedure main:
	tester a = new tester(10)
	a.myvar = 6
	a.addto( 7 )
	another!int b = new another!int()
	b.myvar = 8
	b.addto( 9 )
